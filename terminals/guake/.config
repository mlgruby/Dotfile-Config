[general]
abbreviate-tab-names=false
compat-delete='delete-sequence'
default-shell='/usr/bin/zsh'
display-n=0
gtk-prefer-dark-theme=true
gtk-theme-name='Yaru-dark'
history-size=1000
infinite-history=true
max-tab-name-length=100
mouse-display=true
open-tab-cwd=false
prompt-on-quit=false
quick-open-command-line='code -g %(file_path)s:%(line_number)s'
quick-open-enable=true
restore-tabs-notify=false
restore-tabs-startup=true
save-tabs-when-changed=true
scroll-keystroke=true
scroll-output=false
set-window-title=true
start-at-login=true
start-fullscreen=true
startup-script=''
tab-ontop=true
use-default-font=false
use-login-shell=false
use-popup=false
use-scrollbar=false
use-trayicon=false
window-halignment=0
window-height=50
window-horizontal-displacement=0
window-losefocus=true
window-ontop=false
window-refocus=false
window-tabbar=true
window-valignment=0
window-vertical-displacement=25
window-width=100

[hooks]
show=''

[keybindings/local]
close-tab='disabled'
close-terminal='<Primary><Shift>w'
decrease-height='disabled'
decrease-transparency='disabled'
focus-terminal-down='<Primary><Shift>Down'
focus-terminal-left='<Primary><Shift>Left'
focus-terminal-right='<Primary><Shift>Right'
focus-terminal-up='<Primary><Shift>Up'
increase-height='disabled'
increase-transparency='disabled'
move-tab-left='disabled'
move-tab-right='disabled'
new-tab='<Primary><Shift>t'
new-tab-home='disabled'
next-tab='<Alt>Right'
previous-tab='<Alt>Left'
rename-current-tab='disabled'
search-on-web='disabled'
search-terminal='disabled'
split-tab-horizontal='<Primary>underscore'
split-tab-vertical='<Primary>bar'
switch-tab-last='disabled'
switch-tab1='disabled'
switch-tab10='disabled'
switch-tab2='disabled'
switch-tab3='disabled'
switch-tab4='disabled'
switch-tab5='disabled'
switch-tab6='disabled'
switch-tab7='disabled'
switch-tab8='disabled'
switch-tab9='disabled'
toggle-hide-on-lose-focus='disabled'
toggle-transparency='disabled'
zoom-in='<Primary>equal'
zoom-in-alt='disabled'

[style]
cursor-blink-mode=1
cursor-shape=1

[style/background]
transparency=98

[style/font]
allow-bold=true
bold-is-bright=true
palette='#4c4c4c4c4c4c:#ffff3c3c3c3c:#0d0dbcbc7978:#e5e5e5e51010:#46468281b4b4:#bcbc3f3fbcbc:#1110a8a8cdcd:#e5e5e5e5e5e5:#666666666666:#f1f14c4c4c4c:#2322d1d18b8a:#f5f5f5f54343:#3b3a8e8deaea:#d6d67070d6d6:#2929b8b7dbdb:#e5e5e5e5e5e5:#ffffffffffff:#1d1d1d1d1d1d'
palette-name='Custom'
style='MesloLGS NF 12'
